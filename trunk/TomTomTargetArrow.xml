<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
<Script file="TomTomTargetArrow.lua"/>
	<Frame name="TomTomTargetArrow">
		<Scripts>
			<OnLoad>
				TTTA_OnLoad();
			</OnLoad>
			<OnEvent>
				if (event=="PLAYER_TARGET_CHANGED") then TTTA_OnEventTargetChanged(); end
			</OnEvent>
			<OnUpdate>
				TTTA_OnUpdate(self, elapsed);
			</OnUpdate>
		</Scripts>
	</Frame>
</Ui> 